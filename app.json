{
  "expo": {
    "name": "맛.zip",
    "slug": "matzip",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "matzip",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "맛.zip이 근처 맛집을 추천하고 길찾기 서비스를 제공하기 위해 위치 정보가 필요합니다.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "맛.zip이 근처 맛집을 추천하고 길찾기 서비스를 제공하기 위해 위치 정보가 필요합니다.",
        "LSApplicationQueriesSchemes": [
          "kakaomap",
          "kakaomap"
        ]
      },
      "bundleIdentifier": "me.cher1shrxd.matzip"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "kakaomap"
            }
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "kakaomap"
            }
          ]
        }
      ],
      "package": "me.cher1shrxd.matzip"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "맛.zip이 근처 맛집을 추천하고 길찾기 서비스를 제공하기 위해 위치 정보가 필요합니다."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
